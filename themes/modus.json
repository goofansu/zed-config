{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Modus",
  "author": "Yejun Su",
  "themes": [
    {
      "name": "Modus Light",
      "appearance": "light",
      "style": {
        "editor.foreground": "#000000ff",
        "editor.background": "#ffffffff",
        "syntax": {
          // Emacs: font-lock-variable-name-face
          // cyan
          "attribute": {
            "color": "#005e8b",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-constant-face
          // blue-cooler
          "boolean": {
            "color": "#0000b0",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-comment-face
          // fg-dim
          "comment": {
            "color": "#595959",
            "font_style": "italic",
            "font_weight": null
          },
          // Emacs: font-lock-type-face
          // cyan-cooler
          "constant": {
            "color": "#005f5f",
            "font_style": null,
            "font_weight": 700
          },
          // Emacs: font-lock-constant-face
          // blue-cooler
          "constant.builtin": {
            "color": "#0000b0",
            "font_style": null,
            "font_weight": null
