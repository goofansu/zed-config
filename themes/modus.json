          // Emacs: font-lock-constant-face
          // blue-cooler
          "number": {
            "color": "#0000b0",
            "font_style": null,
            "font_weight": null
          },
          // operator
          "operator": {
            "color": "#000000",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-property-name-face
          // cyan
          "property": {
            "color": "#005e8b",
            "font_style": null,
            "font_weight": null
          },
          // punctuation
          "punctuation.bracket": {
            "color": "#000000",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.delimiter": {
            "color": "#000000",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-variable-name-face
          // cyan
          "punctuation.special": {
            "color": "#005e8b",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-string-face
          // blue-warmer
          "string": {
            "color": "#354fcf",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-string-face
          // blue-warmer
          "string.escape": {
            "color": "#354fcf",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-string-face
          // blue-warmer
          "string.regex": {
            "color": "#354fcf",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-string-face
          // blue-warmer
          "string.special.symbol": {
            "color": "#354fcf",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-function-name-face
          // magenta
          "tag": {
            "color": "#721045",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-type-face
          // cyan-cooler
          "type": {
            "color": "#005f5f",
            "font_style": null,
            "font_weight": 700
          },
          // Emacs: font-lock-type-face
          // cyan-cooler
          "type.super": {
            "color": "#005f5f",
            "font_style": null,
            "font_weight": 700
          },
          // Emacs: font-lock-variable-name-face
          // cyan
          "variable": {
            "color": "#005e8b",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-variable-name-face
          // cyan
          "variable.parameter": {
            "color": "#005e8b",
            "font_style": null,
            "font_weight": null
          },
          // Emacs: font-lock-keyword-face
          // magenta-cooler
          "variable.special": {
            "color": "#531ab6",
            "font_style": null,
            "font_weight": 700
          }
        }
      }
    },
