          "function.method.builtin": {
            "color": "#531ab6",
            "font_style": null,
            "font_weight": 700
          },
          // Emacs: font-lock-keyword-face
          // magenta-cooler
          "keyword": {
            "color": "#531ab6",
            "font_style": null,
            "font_weight": 700
          },
          // Emacs: font-lock-builtin-face
          // magenta-warmer
          "keyword.exception": {
            "color": "#8f0075",
